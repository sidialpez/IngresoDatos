{
  "build": {
    "builder": "nixpacks",
    "nixpacksPlan": {
      "phases": {
        "install": {
          "cmd": "mvn clean install -DskipTests"
        },
        "start": {
          "cmd": "java -jar target/*.jar"
        }
      },
      "environment": {
        "JAVA_VERSION": "21"
      }
    }
  }
}
